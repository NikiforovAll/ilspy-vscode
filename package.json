{
    "name": "msil-decompiler",
    "displayName": "MSIL Decompiler",
    "description": "A decompiler for MSIL assemblies",
    "version": "0.0.1",
    "publisher": "jeremymeng",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:msildecompiler.decompileAssembly"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "msildecompiler.decompileAssembly",
            "title": "Decompile IL Assembly",
            "category": "MSIL Decompiler"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}